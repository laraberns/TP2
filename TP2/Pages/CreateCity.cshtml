@page
@model TP2.Pages.CreateCityModel
@{
	ViewData["Title"] = "Create City";
}
<form method="post">
	<div class="form-group">
		<label class="control-label">Nome da cidade</label>
		<input name="cityName" class="form-control" />
	</div>
	<br />
	<div class="form-group">
		<input type="submit" value="Create" class="btn btn-primary" />
	</div>

</form>

<div class="alert alert-success mt-3">
	Cidade cadastrada: <strong>@Model.CityName</strong>
</div>
